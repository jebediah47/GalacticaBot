# top-most EditorConfig file
root = true

# Apply to all files
[*]
# Use LF for line endings
end_of_line = lf

insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4

# Override for JSON files
[*.json]
indent_size = 2

[*.{csproj,props,targets,xml,config}]
indent_size = 2
max_line_length = 100

